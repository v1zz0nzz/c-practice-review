#include <iostream>
using namespace std;
int main() {
	double num1, num2;
	char op;
	cout << "Enter two number and an operator (+, -, *, or /): ";
	cin >> num1 >> num2 >> op;
	if (op == '+') {
		cout << "Result: " << num1 + num2;
	}
	else if (op == '-') {
		cout << "Result: " << num1 - num2;
	}
	else if (op == '/') {
		cout << "Result: " << num1 / num2;
	}
	else if (op == '*') {
		cout << "Result: " << num1 * num2;
	}
	else {
		cout << "Invalid Operator!";
	}
		
}
